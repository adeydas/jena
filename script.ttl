@prefix acmd:     <http://jena.hpl.hp.com/2006/01/acmd#> .
@prefix rdfs:	  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ja:     <http://jena.hpl.hp.com/2005/11/Assembler#> .

<x> rdf:type acmd:Script ;
    acmd:steps
    (
        _:s1
        _:s2
        _:s3
    ) .

_:s1 rdf:type acmd:Cmd ;
    acmd:main        "sdb.sdbmeta" ;
    # OR 
    ##    acmd:classname  "arq.qparse" ;
    # OR
    ##    acmd:classname  <java:sdb.sdbmeta> ;
    acmd:args ( "--sdb=sdb.ttl" "reset" )
    .

_:s2 rdf:type acmd:Cmd ;
    acmd:main        "sdb.sdbmeta" ;
    acmd:args ( "--sdb=sdb.ttl" "put" "D.ttl" ) 
    .

_:s3 rdf:type acmd:Cmd ;
    acmd:main        "sdb.sdbmeta" ;
    acmd:args ( "--sdb=sdb.ttl" "get" ) 
    .

