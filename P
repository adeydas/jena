#!/bin/bash

STR='query=SELECT+%3Fpredicate+%3Fobject+WHERE+%7B+%3Chttp%3A%2F%2Fdbpedia.org%2Fresource%2FPhilolaus%3E+%3Fpredicate+%3Fobject.+FILTER%28+%28%21isLiteral%28%3Fobject%29%29+%26%26%28+%21regex%28str%28%3Fpredicate%29%2C+%27http%3A%2F%2Fdbpedia.org%2Fproperty%2FrelatedInstance%27%29+%29+%26%26%28+%21regex%28str%28%3Fpredicate%29%2C+%27http%3A%2F%2Fdbpedia.org%2Fproperty%2FwikiPageUsesTemplate%27%29+%29+%26%26%28+%21regex%28str%28%3Fpredicate%29%2C+%27http%3A%2F%2Fwww.w3.org%2F2002%2F07%2Fowl%23sameAs%27%29+%29+%26%26%28+%21regex%28str%28%3Fpredicate%29%2C+%27http%3A%2F%2Fxmlns.com%2Ffoaf%2F0.1%2F%27%29+%29+%26%26%28+%21regex%28str%28%3Fpredicate%29%2C+%27http%3A%2F%2Fwww.w3.org%2F2004%2F02%2Fskos%2Fcore%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fxmlns.com%2Ffoaf%2F0.1%2F%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fupload.wikimedia.org%2Fwikipedia%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fwww4.wiwiss.fu-berlin.de%2Fflickrwrappr%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fdbpedia.org%2Fresource%2FTemplate%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fupload.wikimedia.org%2Fwikipedia%2Fcommons%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fwww.w3.org%2F2006%2F03%2Fwn%2Fwn20%2Finstances%2Fsynset%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fdbpedia.org%2Fresource%2FCategory%3A%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fwww.w3.org%2F2004%2F02%2Fskos%2Fcore%27%29+%29+%26%26%28+%21regex%28str%28%3Fobject%29%2C+%27http%3A%2F%2Fwww.geonames.org%27%29+%29%29.%7D+&default-graph-uri=http%3A%2F%2Fdbpedia.org'

wget -O - --post-data="$STR" \
    --header="Accept: application/sparql-results+xml" \
    'http://dbpedia.openlinksw.com:8890/sparql'
