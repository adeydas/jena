PREFIX  rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX  rdfs:   <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pd: <http://example/>

SELECT DISTINCT ?dc
{


    ?x  a/rdfs:subClassOf*      pd:Entry ;
        pd:dc   ?dc .


    ?z  rdf:type       pd:Entry ;
        pd:dc   ?dc .
}

