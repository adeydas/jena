PREFIX  :     <http://example/>
PREFIX  foaf:   <http://xmlns.com/foaf/0.1/>

SELECT ?A ?y
{
  { LET(?A := 1)
     :a foaf:knows{2} ?y
  }
UNION
{
   LET(?A := 2)
     :a foaf:knows* ?y 
}
}

