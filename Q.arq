PREFIX dc: <http://purl.org/dc/elements/1.1/>

SELECT ?book ?who
{
    SERVICE <http://sparql.org/books>
    { ?book dc:title ?title }
    SERVICE <http://sparql.org/books>
    { ?book dc:creator ?who . FILTER isLiteral(?who) }
}
