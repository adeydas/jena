PREFIX  rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX  rdfs:   <http://www.w3.org/2000/01/rdf-schema#>
PREFIX  :       <http://example.org/>

construct {
:Result rdf:type rdfs:Class .
_:v rdf:type :Result .
_:v :hasTotal ?c .
}
WHERE
{
 {select (sum(?d) as ?c)
where {
?ms rdf:type :marksheet .
?ms :hasMark ?m .
?m :hasData ?d .
}}
}