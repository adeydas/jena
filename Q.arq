PREFIX fb: <http://rdf.freebase.com/ns/>
SELECT ?actor ?name
FROM fb:en.blade_runner
{
  fb:en.blade_runner fb:film.film.starring ?personUUID
  FETCH ?personUUID
  GRAPH ?personUUID { ?personUUID fb:film.performance.actor ?actor }
  FETCH ?actor
  GRAPH ?actor { ?actor fb:type.object.name ?name }
}
ORDER BY ?actor
