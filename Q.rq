PREFIX dbpprop: <http://dbpedia.org/property/>
PREFIX edu: <http://education.data.gov.uk/ontology/school#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX admingeo: <http://www.ordnancesurvey.co.uk/ontology/admingeo/>
PREFIX census-geo: <http://statistics.data.gov.uk/ontology/census-geo/>

SELECT ?onscode ?name ?casName
WHERE {
 ?school edu:censusAreaStatisticWard [ rdfs:label ?casName ]

 OPTIONAL {
   GRAPH <http://education.data.gov.uk/onscodes> {
     ?thing rdfs:label ?casName;
            a census-geo:CAS-Ward;
            admingeo:hasCenusCode ?onscode;
            .
   }
 }
  FILTER (!bound(?thing))
} LIMIT 100
