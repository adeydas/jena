PREFIX   :      <http://example/>
PREFIX  dc10:     <http://purl.org/dc/elements/1.0/>
PREFIX  dc11:     <http://purl.org/dc/elements/1.1/>
PREFIX  rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?name
WHERE {
   ?a dc10:title "thing" .
   OPTIONAL { ?a dc10:creator ?name }
   OPTIONAL { ?a dc11:creator ?c .
              ?c rdf:value ?name }
}
