PREFIX my: <> 
SELECT ?result 
WHERE {
{
    SELECT ?result ?number 
    {
        ?this my:x ?number .
        ?result my:a ?number .
    } LIMIT 1
}
OPTIONAL
{
   FILTER(!bound(?number))
   { SELECT ?result {
       ?this my:y ?acct .
       ?result my:b ?acct .
    } LIMIT 1
   }
}
