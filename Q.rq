PREFIX vcard:      <http://www.w3.org/2001/vcard-rdf/3.0#>
PREFIX elements:         <http://purl.org/dc/elements/1.1/>

SELECT ?title
             WHERE  {
              ?uri vcard:FN "John Smith-7" .
              ?bookURI elements:creator ?uri . 
              ?bookURI elements:title ?title . 
             }
