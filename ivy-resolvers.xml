<ivysettings>
  <!-- Resolvers for dependences -->
  <resolvers>
    <chain name="resolver-chain">
      <!-- Jena development repository : not sync'ed with maven central -->
      <ibiblio name="jena-dev" root="http://openjena.org/repo-dev" m2compatible="true" useMavenMetadata="false"/>

      <!-- Jena publish repository, sync'ed to maven central -->
      <!-- <ibiblio name="jena" root="http://openjena.org/repo" m2compatible="true" useMavenMetadata="false"/> -->

      <ibiblio name="maven2" m2compatible="true" />
    </chain>
  </resolvers>
</ivysettings>
