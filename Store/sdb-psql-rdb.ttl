@prefix acmd:     <http://jena.hpl.hp.com/2006/01/acmd#> .
@prefix rdfs:	  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ja:     <http://jena.hpl.hp.com/2005/11/Assembler#> .

# better to have a specific ModelRDB section using Jena assembler?

_:c rdf:type acmd:SDBConnection ;
    acmd:sdbType       "postgresql" ;    # Needed for JDBC URL
    acmd:sdbHost       "localhost" ;
    acmd:sdbName       "jenatest" ;
    acmd:driver        "org.postgresql.Driver" ;
    acmd:rdbType       "PostgreSQL" ;
    .

[] rdf:type acmd:Store ;
    acmd:layout     "RDB" ;
    #acmd:rdbModeName "" ; # Default model.
    acmd:connection _:c ;
.
