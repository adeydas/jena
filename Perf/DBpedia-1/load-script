#!/bin/bash

# Run in TDBROOT

TDBROOT="${TDBROOT:-$HOME/Projects/TDB}"
DATASETS="${DATASETS:-$HOME/Datasets}"
#DB_DESC="$TDBROOT/DB/DBpediaBM/dataset-dbp.ttl"
DB_DIR=DB
LOCATION="--location $DB_DIR"

#PARA="${PARA:---parallel}"

tdbclean $DB_DIR
tdbloader $LOCATION $PARA \
    $DATASETS/DBPediaBM/dbp-infoboxes.nt \
    $DATASETS/DBPediaBM/dbp-geocoordinates.nt \
    $DATASETS/DBPediaBM/dbp-homepages.nt \
    | tee LOG
